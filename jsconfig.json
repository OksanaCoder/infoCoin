{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "baseUrl": "src",
    "jsx": "react",
    "paths": {
      "@styles/*": ["styles/*"],
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@containers/*" : ["containers/*"],
      "@pages/*" : ["pages/*"],
      "@fonts/*" : ["fonts/*"],
      "@helpers/*" : ["helpers/*"],
      "@services/*" : ["services/*"],
      "@redux/*" : ["redux/*"],
      "@redux_actions/*" : ["redux/actions/*"],
      "@reducer" : ["redux/reducers/index.js"],
      "@actions/*" : ["actions/*"]
    }
  },
  "include": [
    "src"
  ]
}